{
  "hosting": {
    "target": "prod",
    "cleanUrls": true,
    "trailingSlash": true,
    "public": "build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/core-ui/**",
        "destination": "/core-ui/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
